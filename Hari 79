package hari71;
import java.util.ArrayList;

public class Main {
    public static void main(String[] args) {
        
        ArrayList<Integer> numbers = new ArrayList<>();
        numbers.add(5);
        numbers.add(10);
        numbers.add(15);
        System.out.println("Elemen-elemen ArrayList: " + numbers);
        int firstElement = numbers.get(2);
        System.out.println("Elemen pertama: " + firstElement);
    }
}
