package ISC;


public class Hari10 {
    public static void main(String[] args) {
        int[] angka = {-80, -85, -50, -25, -90}; 
        double total = 0;

        for (int i = 0; i < angka.length; i++) {
            total += angka[i];
        }

        double rataRata = total / angka.length;

        System.out.println("Rata-rata angka adalah: " + rataRata);
    }
    
}
