package hari74;

public class dinah {
    private String name;
    private int age;

    public dinah(String name, int age) {
        this.name = name;
        this.age = age;
    }

    public void introduce() {
        System.out.println("Hello, my name is " + name + " and I am " + age + " years old.");
    }

    public static void main(String[] args) {
        dinah dinahObj = new dinah("Dinah", 18);
        dinahObj.introduce();
    }
}
