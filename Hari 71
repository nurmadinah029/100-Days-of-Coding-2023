package com.mycompany.projrk01;

public class kotak {

    public String nama, nim;
    public int umur;

    public String getNama() {
        return nama;
    }

    public void setNama(String nama) {
        this.nama = nama;
    }

    public String getNim() {
        return nim;
    }

    public void setNim(String nim) {
        this.nim = nim;
    }

    public int getUmur() {
        return umur;
    }

    public void setUmur(int umur) {
        this.umur = umur;
    }
}





package com.mycompany.projrk01;
public class Hari71{
   public static void main(String[] args) {
        kotak dataData = new kotak();
        dataData.setNama("NURMADINAH");
        System.out.println(dataData.getNama());
        dataData.setNim("D0223001");
        System.out.println(dataData.getNim());
        dataData.setUmur(18);
        System.out.println(dataData.getUmur());
    }
}
