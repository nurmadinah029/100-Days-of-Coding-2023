package hari89;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
   Scanner im = new Scanner(System.in);
   System.out.print("masukkan angka: ");    
   int angka =im.nextInt();
   if (angka %5==0 ) {
      System.out.println("angka + 5: "+5);
   }else if (angka %3==0+3) {
      System.out.println("angka + 3: "+3);
   }if (angka == 50) {
      System.out.println("colll");
         
      }
   }
   
   }
