package ISC;
public class Hari55 {
    public static void main(String[] args) {
        String nama = "John";
        int umur = 25;
        
        System.out.println("Nama: " + nama);
        System.out.println("Umur: " + umur);
        
        String hasil = sapa(nama);
        System.out.println(hasil);
    }
    
    static String sapa(String nama) {
        return "Halo, " + nama + "! Selamat datang!";
    }
}

