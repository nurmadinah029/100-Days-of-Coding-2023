public class Main {
    
    public static void main(String[] args) {
        System.out.println("hay semua");
    
        String name = "Nurmadinah";
        System.out.println("Nama: " + name);

        Integer age = Integer.valueOf(25);
        System.out.println("Usia: " + age);

        Double height = Double.valueOf(175.5);
        System.out.println("Tinggi: " + height);

        // Menambahkan tipe data non-primitif
        Boolean isStudent = Boolean.valueOf(true);
        System.out.println("Status Mahasiswa: " + isStudent);

        Character firstInitial = Character.valueOf('N');
        System.out.println("Inisial Pertama: " + firstInitial);

        Float gpa = Float.valueOf(3.75f);
        System.out.println("IPK: " + gpa);

        Long studentID = Long.valueOf(20230001L);
        System.out.println("ID Mahasiswa: " + studentID);

        Short semester = Short.valueOf((short) 2);
        System.out.println("Semester: " + semester);
}
}
