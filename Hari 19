    int umur = 18;
    float beratBadan = 50.5f;
    double tinggiBadan = 150.5;
        System.out.println("Nama =" + Nama+"\n" + "Prodi =" + Prodi+"\n"+ "umur =" + umur+"\n" + "berat badan =" + beratBadan +"\n"+ "tinggi badan =" + tinggiBadan );
    
 //Update variabel
   nama = "nurmadinah";
   NoAlamat = "02";
   angkatan = 2023;
   JenisKelamin = 'p';
   Ipk = 4.5;
        System.out.println(nama+"\n"+NoAlamat+"\n"+angkatan+"\n"+JenisKelamin+"\n"+Ipk);
          
    
